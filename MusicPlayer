package music

import java.util.Objects


object MusicPlayer {
    fun playMusic(inputStream: String?) {
        Your Client start up.musicHelper.setStream(Objects::class.java.getResourceAsStream(inputStream))
        Your Client start up.musicHelper.start()

    }

    fun stopMusic() {
        Your Client start up.musicHelper.stop()
    }

    val isCompleted: Boolean
        get() = Your Client.musicHelper.isCompleted()
}/* add this start side musicHelper = new MusicHelper(); this is for java*/
